{
  "version": 2,
  "buildCommand": "npm install && npm run build",
  "installCommand": "npm install",
  "outputDirectory": "frontend/.next",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://$BACKEND_URL/api/:path*"
    }
  ],
  "github": {
    "silent": true
  }
}
